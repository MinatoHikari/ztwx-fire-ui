{
  "name": "ztwx-fire-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "compiler-manager": "tsc -d --outDir lib/ztwManager -m commonjs src/ztwManager/**.ts",
    "compiler-lib": "tsc -d --outFile src/ztwComponents/modules/modules.ts",
    "compiler-http": "tsc -d --project src/ztwHttp --outDir http",
    "compiler-session": "tsc -d --project src/ztwSession --outDir session",
    "watch-src": "node lib/ztwManager/templates-manage/watch-cli.js src",
    "webfont": "cmWebfont start",
    "build:lib": "vue-cli-service build --target lib --name index --dest lib/package --inline-vue false src/ztwComponents/modules/modules.ts"
  },
  "typings": "lib/packageTypes/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/package/index.umd.js",
  "dependencies": {
    "@ztwx/webfont": "^0.0.1-alpha",
    "cache-ajax": "^3.0.1",
    "core-js": "^3.6.4",
    "rxjs": "^6.5.4",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "3.0.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-typescript": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "chokidar": "^3.3.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "typescript": "~3.7.5",
    "vue-template-compiler": "^2.6.11"
  }
}
